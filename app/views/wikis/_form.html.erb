<%= form_for @wiki do |f| %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title,
          class: 'form-control',
          placeholder: "Enter title here" %>
  </div>
  <div class="form-group">
    <%= f.label :body %>
    <%= f.text_area :body,
          rows: 8,
          class: 'form-control',
          placeholder: "Write review here" %>
  </div>
  <div class = "form-group">
    <%= f.label :private, class: 'check_box' do %>
      <%= f.check_box :private %> Private?
    <% end %>
  </div>
    <%= f.submit "Save", class: 'btn btn-success' %>
<% end %>
