<h1>All Reviews <%= link_to "Create New Wiki", new_wiki_path, class: 'btn btn-success' %></h1>

<% @wikis.each do |wiki| %>
  <% if current_user.premium? || current_user.admin? %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
          <%= link_to wiki.title, wiki %>
      </h4>
    </div>
  </div>
  <% elsif wiki.private != true %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
          <%= link_to wiki.title, wiki %>
      </h4>
    </div>
  </div>
  <% end %>
<% end %>
